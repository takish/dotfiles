{
  "obsidian": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "mcp-obsidian",
      "/Users/takish/Library/Mobile Documents/iCloud~md~obsidian/Documents/myvault"
    ],
    "env": {}
  },
  "chrome-devtools": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "chrome-devtools-mcp@latest"
    ],
    "env": {}
  },
  "figma-mcp": {
    "type": "http",
    "url": "http://127.0.0.1:3845/mcp"
  },
  "firecrawl": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "firecrawl-mcp"
    ],
    "env": {
      "FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}"
    }
  },
  "note-post-mcp": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "@gonuts555/note-post-mcp@latest"
    ],
    "env": {
      "NOTE_POST_MCP_STATE_PATH": "${NOTE_POST_MCP_STATE_PATH}"
    }
  }
}
